namespace "opd-test"."opd-test-db";

context tables {

    /*@@layout{"layoutInfo":{}}*/
    entity Railway {
        InboundWord : String(100);
        Operator    : String(100);
        odptRailway : String(100);
        odptStation : String(100);
        odptIndex   : Integer;
    };

    /*@@layout{"layoutInfo":{}}*/
    entity Station {
        InboundWord  : String(100);
        odptStation  : String(100);
        odptOperator : String(100);
        odptRailway  : String(100);
    };

    /*@@layout{"layoutInfo":{}}*/
    entity RailDirection {
        Railway           : String(100);
        odptRailDirection : String(100);
        UpDown            : String(100);
        InboundWord       : String(100);
    };

    /*@@layout{"layoutInfo":{}}*/
    entity RailwayTemp {
        odptRailway : String(100);
        InboundWord : String(100);
    };

    /*@@layout{"layoutInfo":{}}*/
    entity StationTemp {
        odptRailway : String(100);
        odptStation : String(100);
        InboundWord : String(100);
    };

    /*@@layout{"layoutInfo":{}}*/
    entity RailDirectionTemp {
        odptRailDirection : String(100);
        InboundWord       : String(100);
    };
};